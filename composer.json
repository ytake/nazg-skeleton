{
  "name": "nazg/skeleton",
  "description": "Action-Domain-Responder (ADR) Micro framework for HHVM/Hack",
  "type": "project",
  "keywords": [
    "hhvm",
    "hack",
    "framework",
    "adr",
    "middleware",
    "request",
    "response",
    "route",
    "xhp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "hhvm": "^4.35",
    "hhvm/hsl": "^4.25",
    "hhvm/hsl-experimental": "^4.25",
    "hhvm/hhvm-autoload": "^3.0",
    "nazg/framework": "^0.9",
    "ytake/hh-config-aggregator": "^0.7",
    "ytake/hackdotenv": "^0.6",
    "facebook/xhp-lib": "^3.1"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0",
    "facebook/fbexpect": "^2.7",
    "hhvm/hhast": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
